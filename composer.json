{
  "name": "ngiraud/laravel-cookies-analytics",
  "description": "Add Google Analytics and cookie consent to your application.",
  "keywords": [
    "laravel",
    "cookies",
    "analytics",
    "laravel-cookies-analytics"
  ],
  "homepage": "https://github.com/ngiraud/laravel-cookies-analytics",
  "license": "MIT",
  "authors": [
    {
      "name": "Nicolas Giraud",
      "email": "contact@ngiraud.me",
      "homepage": "https://www.nicolasgiraud.fr",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.1",
    "illuminate/support": "~5.5.0|~5.6.0|~5.7.0|~5.8.0|^6.0"
  },
  "autoload": {
    "psr-4": {
      "NGiraud\\CookiesAnalytics\\": "src"
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "NGiraud\\CookiesAnalytics\\CookiesAnalyticsServiceProvider"
      ]
    }
  }
}
